{
  "name": "netspecter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "src/main/main.js",
  "scripts": {
    "build:preload": "esbuild src/main/preload.js --bundle --format=cjs --outfile=src/main/preload.cjs --external:electron",
    "dev": "npm run build:preload && concurrently -k \"vite\" \"cross-env NODE_ENV=development electron .\"",
    "build": "npm run build:preload && vite build",
    "preview": "vite preview",
    "test": "vitest run --dir ./test"
  },
  "devDependencies": {
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "electron": "^40.6.0",
    "vite": "^7.3.1",
    "vitest": "^4.0.18"
  },
  "dependencies": {
    "electron-store": "^11.0.2",
    "electron-updater": "^6.8.3",
    "local-network-scanner": "^0.2.0",
    "ping": "^1.0.0",
    "split2": "^4.2.0"
  }
}